	file = {PubMed Central Full Text PDF:/home/vincent/Dokumente/Bibliographie/storage/YJBW5LSM/Hajdin et al. - 2010 - On the significance of an RNA tertiary structure p.pdf:application/pdf}
}

@article{weeks_advances_2010,
