	note = {Publisher: National Academy of Sciences
Section: Research Article},
	file = {Full Text PDF:/home/vincent/Dokumente/Bibliographie/storage/RZSWCUI3/Breaker und Joyce - 1994 - Emergence of a replicating species from an in vitr.pdf:application/pdf;Snapshot:/home/vincent/Dokumente/Bibliographie/storage/UHSHHBZ6/6093.html:text/html}
}

@article{schuster_sequences_1994,
	title = {From sequences to shapes and back: a case study in {RNA} secondary structures},
	volume = {255},
	url = {https://royalsocietypublishing.org/doi/10.1098/rspb.1994.0040},
	doi = {10.1098/rspb.1994.0040},
	shorttitle = {From sequences to shapes and back},
	abstract = {{RNA} folding is viewed here as a map assigning secondary structures to sequences. At fixed chain length the number of sequences far exceeds the number of structures. Frequencies of structures are highly non-uniform and follow a generalized form of Zipf’s law: we find relatively few common and many rare ones. By using an algorithm for inverse folding, we show that sequences sharing the same structure are distributed randomly over sequence space. All common structures can be accessed from an arbitrary sequence by a number of mutations much smaller than the chain length. The sequence space is percolated by extensive neutral networks connecting nearest neighbours folding into identical structures. Implications for evolutionary adaptation and for applied molecular evolution are evident: finding a particular structure by mutation and selection is much simpler than expected and, even if catalytic activity should turn out to be sparse in the space of {RNA} structures, it can hardly be missed by evolutionary processes.},
	pages = {279--284},
	number = {1344},
	journaltitle = {Proceedings of the Royal Society of London. Series B: Biological Sciences},
	shortjournal = {Proceedings of the Royal Society of London. Series B: Biological Sciences},
	author = {Schuster, Peter and Fontana, Walter and Stadler, Peter F. and Hofacker, Ivo L.},
	urldate = {2020-04-14},
	date = {1994-03-22},
	note = {Publisher: Royal Society},
	file = {Full Text PDF:/home/vincent/Dokumente/Bibliographie/storage/SBH9C72R/Schuster et al. - 1994 - From sequences to shapes and back a case study in.pdf:application/pdf;Snapshot:/home/vincent/Dokumente/Bibliographie/storage/2DEJMMU8/rspb.1994.html:text/html}
}

