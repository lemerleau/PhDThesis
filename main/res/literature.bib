	number = {10},
	journaltitle = {Bioinformatics},
	shortjournal = {Bioinformatics},
	author = {Mückstein, Ulrike and Tafer, Hakim and Hackermüller, Jörg and Bernhart, Stephan H. and Stadler, Peter F. and Hofacker, Ivo L.},
