	note = {Company: Cold Spring Harbor Laboratory Press
Distributor: Cold Spring Harbor Laboratory Press
Institution: Cold Spring Harbor Laboratory Press
Label: Cold Spring Harbor Laboratory Press
Publisher: Cold Spring Harbor Lab},
	keywords = {{RNA} secondary structure, base-pair metric, centroid structure, cluster, consensus structure, k-means clustering, medioid structure, minimum free energy structure, relaxed base-pair score, representative structure, spectral clustering},
	file = {Full Text PDF:/home/vincent/Dokumente/Bibliographie/storage/327XZ7IL/Agius et al. - 2010 - Comparing RNA secondary structures using a relaxed.pdf:application/pdf;Snapshot:/home/vincent/Dokumente/Bibliographie/storage/73KXKMSS/865.html:text/html}
