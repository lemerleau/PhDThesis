	keywords = {Directed Molecular Evolution, High-Throughput Nucleotide Sequencing, {RNA}, Catalytic, Sequence Analysis, {DNA}},
	file = {Volltext:/home/vincent/Dokumente/Bibliographie/storage/86MRITLW/Ameta et al. - 2014 - Next-generation sequencing reveals how RNA catalys.pdf:application/pdf}
