	langid = {english},
	note = {Number: 6120
Publisher: Nature Publishing Group},
	file = {Full Text PDF:/home/vincent/Dokumente/Bibliographie/storage/UPQI6ZXR/Zielinski und Orgel - 1987 - Autocatalytic synthesis of a tetranucleotide analo.pdf:application/pdf;Snapshot:/home/vincent/Dokumente/Bibliographie/storage/EFBD88UN/327346a0.html:text/html}
}

