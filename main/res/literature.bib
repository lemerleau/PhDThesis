	shortjournal = {J. Am. Chem. Soc.},
	author = {Zhang, Xinyue and Xu, Xiaojun and Yang, Zhiyu and Burcke, Andrew J. and Gates, Kent S. and Chen, Shi-Jie and Gu, Li-Qun},
