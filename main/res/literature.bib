@article{devoe_stability_1962,
	title = {The stability of helical polynucleotides: Base contributions},
	volume = {4},
	issn = {0022-2836},
	url = {https://www.sciencedirect.com/science/article/pii/S0022283662801053},
	doi = {10.1016/S0022-2836(62)80105-3},
	shorttitle = {The stability of helical polynucleotides},
	abstract = {Estimates have been made of the main contributions of the heterocyclic bases of {DNA} to the free energies of the two configurations in solution, the two-stranded helix and the single-strand random coil. The magnitude and directions of the dipole moments of the adenine, thymine, guanine and cytosine base groups have been calculated by a semi-empirical molecular orbital treatment. Dipole—dipole, dipole—induced-dipole and London force interactions among the bases in the helix are large, and make the free energy of the helix depend on the base composition and sequence. The helix stability (helix negative free energy) is proportional to the guanine + cytosine content. A comparison of calculated base-pair interactions with the nearest-neighbor base frequency data of Josse, Kaiser \& Kornberg (1961) suggests that the base sequence in natural {DNA} may be influenced by the free energy. The free energy of the coil is difficult to estimate ; it tends to be more negative from base—solvent interactions for a greater guanine—cytosine content. The choice of solvent determines the magnitude of these interactions and thus determines how the melting temperature of the helix will depend on the base composition. In a solvent such as water, in which significant base—base interactions exist in the coil, the coil free energy depends on the base sequence and London forces may cause stacking of the bases into ordered arrays. The hydrogen bond energy of the bases and the strain energy in the helix probably contribute little to the enthalpy change of the helix—coil transition, although they ensure specific base-pairing in the helix. The net contribution of configurational and solvent entropy changes to the free energy change of the helix—coil transition is also probably small.},
	pages = {500--517},
	number = {6},
	journaltitle = {Journal of Molecular Biology},
	shortjournal = {Journal of Molecular Biology},
	author = {{DeVoe}, Howard and Tinoco, Ignacio},
	urldate = {2021-04-14},
	date = {1962-06-01},
	langid = {english},
	file = {ScienceDirect Full Text PDF:/home/vincent/Dokumente/Bibliographie/storage/I9D2RYEW/DeVoe und Tinoco - 1962 - The stability of helical polynucleotides Base con.pdf:application/pdf;ScienceDirect Snapshot:/home/vincent/Dokumente/Bibliographie/storage/K6QICR25/S0022283662801053.html:text/html}
}

