	keywords = {{RNA} secondary structure, partition function, free energy minimization, nearest-neighbor model},
	file = {Full Text PDF:/home/vincent/Dokumente/Bibliographie/storage/TXAGDT9V/Lu et al. - 2009 - Improved RNA secondary structure prediction by max.pdf:application/pdf;Snapshot:/home/vincent/Dokumente/Bibliographie/storage/WLSF86HL/1805.html:text/html}
