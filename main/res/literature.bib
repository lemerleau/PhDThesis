@inproceedings{amman_trouble_2013,
	location = {Cham},
	title = {The Trouble with Long-Range Base Pairs in {RNA} Folding},
	isbn = {978-3-319-02624-4},
	doi = {10.1007/978-3-319-02624-4_1},
	series = {Lecture Notes in Computer Science},
	abstract = {{RNA} prediction has long been struggling with long-range base pairs since prediction accuracy decreases with base pair span. We analyze here the empirical distribution of base pair spans in large collection of experimentally known {RNA} structures. Surprisingly, we find that long-range base pairs are overrepresented in these data. In particular, there is no evidence that long-range base pairs are systematically overpredicted relative to short-range interactions in thermodynamic predictions. This casts doubt on a recent suggestion that kinetic effects are the cause of length-dependent decrease of predictability. Instead of a modification of the energy model we advocate a modification of the expected accuracy model for {RNA} secondary structures. We demonstrate that the inclusion of a span-dependent penalty leads to improved maximum expected accuracy structure predictions compared to both the standard {MEA} model and a modified folding algorithm with an energy penalty function. The prevalence of long-range base pairs provide further evidence that {RNA} structures in general do not have the so-called polymer zeta property. This has consequences for the asymptotic performance for a large class of sparsified {RNA} folding algorithms.},
	pages = {1--11},
	booktitle = {Advances in Bioinformatics and Computational Biology},
	publisher = {Springer International Publishing},
	author = {Amman, Fabian and Bernhart, Stephan H. and Doose, Gero and Hofacker, Ivo L. and Qin, Jing and Stadler, Peter F. and Will, Sebastian},
	editor = {Setubal, João C. and Almeida, Nalvo F.},
	date = {2013},
	langid = {english},
	keywords = {{RNA} folding, long-range base pair, polymer zeta property, prediction accuracy},
	file = {Springer Full Text PDF:/home/vincent/Dokumente/Bibliographie/storage/4GL6YMWC/Amman et al. - 2013 - The Trouble with Long-Range Base Pairs in RNA Fold.pdf:application/pdf}
}

@article{russell_exploring_2002,
	title = {Exploring the folding landscape of a structured {RNA}},
	volume = {99},
