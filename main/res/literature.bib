	keywords = {Nucleic Acid Conformation, Ribosomes, {RNA}, Ribosomal},
	file = {Akzeptierte Version:/home/vincent/Dokumente/Bibliographie/storage/A2BDHAVT/Zhang et al. - 2015 - Mimicking Ribosomal Unfolding of RNA Pseudoknot in.pdf:application/pdf}
