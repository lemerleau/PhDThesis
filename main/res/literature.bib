	date = {2007-07-01},
	pmid = {17646296},
	keywords = {Algorithms, Models, Molecular, Base Sequence, Nucleic Acid Conformation, {RNA}, Computer Simulation, Sequence Analysis, {RNA}, Molecular Sequence Data, Models, Chemical},
	file = {Volltext:/home/vincent/Dokumente/Bibliographie/storage/392ZPHXA/Andronescu et al. - 2007 - Efficient parameter estimation for RNA secondary s.pdf:application/pdf}
}

