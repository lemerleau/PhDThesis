	number = {4},
	journaltitle = {Bioinformatics},
	shortjournal = {Bioinformatics},
	author = {Hamada, Michiaki and Kiryu, Hisanori and Sato, Kengo and Mituyama, Toutai and Asai, Kiyoshi},
	urldate = {2021-03-26},
	date = {2009-02-15},
	file = {Full Text PDF:/home/vincent/Dokumente/Bibliographie/storage/UZWD3E46/Hamada et al. - 2009 - Prediction of RNA secondary structure using genera.pdf:application/pdf;Snapshot:/home/vincent/Dokumente/Bibliographie/storage/MWUDCBRS/248190.html:text/html}
}

@article{sato_rna_2021,
