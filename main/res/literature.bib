	pmcid = {PMC3331698},
	file = {PubMed Central Full Text PDF:/home/vincent/Dokumente/Bibliographie/storage/5YDVXA2E/Robertson und Joyce - 2012 - The Origins of the RNA World.pdf:application/pdf}
}

