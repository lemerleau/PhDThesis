	keywords = {Algorithms, Models, Molecular, Reproducibility of Results, Base Sequence, Nucleic Acid Conformation, {RNA}, Base Pairing, Sequence Analysis, {RNA}, Molecular Sequence Data, Sequence Alignment, Sequence Homology, Nucleic Acid, Feedback, Sensitivity and Specificity},
	file = {Volltext:/home/vincent/Dokumente/Bibliographie/storage/AZ9CFSYJ/Ruan et al. - 2004 - An iterated loop matching approach to the predicti.pdf:application/pdf;Volltext:/home/vincent/Dokumente/Bibliographie/storage/ZXB9HDDR/Ruan et al. - 2004 - An iterated loop matching approach to the predicti.pdf:application/pdf}
