	langid = {english},
	file = {ScienceDirect Full Text PDF:/home/vincent/Dokumente/Bibliographie/storage/9XYDQ5AZ/Schneider et al. - 1986 - Information content of binding sites on nucleotide.pdf:application/pdf;ScienceDirect Snapshot:/home/vincent/Dokumente/Bibliographie/storage/WTB6S2G6/0022283686901658.html:text/html}
}

