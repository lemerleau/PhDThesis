	date = {2018-01-15},
	pmid = {29334887},
	pmcid = {PMC5769536},
	keywords = {Algorithms, Computational Biology, Models, Molecular, Nucleic Acid Conformation, {RNA}, Pseudoknot, Databases, Genetic, Secondary structure, Bi-objective, Integer programming, Optimal solutions, Sub-optimal solutions, Time Factors},
