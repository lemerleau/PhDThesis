	number = {14},
	journaltitle = {Bioinformatics},
	shortjournal = {Bioinformatics},
	author = {Kucharík, Marcel and Hofacker, Ivo L. and Stadler, Peter F. and Qin, Jing},
	urldate = {2021-05-02},
	date = {2014-07-15},
	file = {Full Text PDF:/home/vincent/Dokumente/Bibliographie/storage/XBESGIUY/Kucharík et al. - 2014 - Basin Hopping Graph a computational framework to .pdf:application/pdf;Snapshot:/home/vincent/Dokumente/Bibliographie/storage/GK9Z8SCL/2390590.html:text/html}
}

