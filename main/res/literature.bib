	date = {2015-09-03},
	pmid = {26335276},
	pmcid = {PMC4559319},
	file = {PubMed Central Full Text PDF:/home/vincent/Dokumente/Bibliographie/storage/YT3E3GXI/Taneda - 2015 - Multi-objective optimization for RNA design with m.pdf:application/pdf}
}

