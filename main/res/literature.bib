	pmid = {28049135},
	pmcid = {PMC6018860},
	keywords = {Animals, Algorithms, Computational Biology, Models, Molecular, Nucleic Acid Conformation, {RNA}, Software, {RNA} Folding, Humans},
	file = {PubMed Central Full Text PDF:/home/vincent/Dokumente/Bibliographie/storage/NBZG6ZCB/Churkin et al. - 2017 - Design of RNAs comparing programs for inverse RNA.pdf:application/pdf;Volltext:/home/vincent/Dokumente/Bibliographie/storage/RCLZH437/Churkin et al. - 2018 - Design of RNAs comparing programs for inverse RNA.pdf:application/pdf}
