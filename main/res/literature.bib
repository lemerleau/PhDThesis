@article{peng_solving_2021,
	title = {Solving Sparse Linear Systems Faster than Matrix Multiplication},
	url = {http://arxiv.org/abs/2007.10254},
	abstract = {Can linear systems be solved faster than matrix multiplication? While there has been remarkable progress for the special cases of graph structured linear systems, in the general setting, the bit complexity of solving an \$n {\textbackslash}times n\$ linear system \$Ax=b\$ is \${\textbackslash}tilde\{O\}(n{\textasciicircum}{\textbackslash}omega)\$, where \${\textbackslash}omega {\textless} 2.372864\$ is the matrix multiplication exponent. Improving on this has been an open problem even for sparse linear systems with poly\$(n)\$ condition number. In this paper, we present an algorithm that solves linear systems in sparse matrices asymptotically faster than matrix multiplication for any \${\textbackslash}omega {\textgreater} 2\$. This speedup holds for any input matrix \$A\$ with \$o(n{\textasciicircum}\{{\textbackslash}omega -1\}/{\textbackslash}log({\textbackslash}kappa(A)))\$ non-zeros, where \${\textbackslash}kappa(A)\$ is the condition number of \$A\$. For poly\$(n)\$-conditioned matrices with \${\textbackslash}tilde\{O\}(n)\$ nonzeros, and the current value of \${\textbackslash}omega\$, the bit complexity of our algorithm to solve to within any \$1/{\textbackslash}text\{poly\}(n)\$ error is \$O(n{\textasciicircum}\{2.331645\})\$. Our algorithm can be viewed as an efficient, randomized implementation of the block Krylov method via recursive low displacement rank factorizations. It is inspired by the algorithm of [Eberly et al. {ISSAC} `06 `07] for inverting matrices over finite fields. In our analysis of numerical stability, we develop matrix anti-concentration techniques to bound the smallest eigenvalue and the smallest gap in eigenvalues of semi-random matrices.},
	journaltitle = {{arXiv}:2007.10254 [cs, math]},
	author = {Peng, Richard and Vempala, Santosh},
	urldate = {2021-03-10},
	date = {2021-01-07},
	eprinttype = {arxiv},
	eprint = {2007.10254},
	keywords = {Computer Science - Data Structures and Algorithms, Mathematics - Numerical Analysis},
	file = {arXiv Fulltext PDF:/home/vincent/Dokumente/Bibliographie/storage/DRXMF963/Peng und Vempala - 2021 - Solving Sparse Linear Systems Faster than Matrix M.pdf:application/pdf;arXiv.org Snapshot:/home/vincent/Dokumente/Bibliographie/storage/MVUAPQP2/2007.html:text/html}
}

